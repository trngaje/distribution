mkimage_dtb() {
  echo "image: copying device trees..."
  for dtb in ${DEVICE_DTB[@]}; do
    echo "image: not copying ${dtb}.dtb..."
    mcopy ${RELEASE_DIR}/3rdparty/bootloader/${dtb}.dtb ::
  done
}

mkimage_dtb
