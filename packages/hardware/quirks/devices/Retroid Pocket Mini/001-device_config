cat <<EOF >/storage/.config/profile.d/001-device_config
# Device Features
DEVICE_HAS_TOUCHSCREEN="true"
DEVICE_PLAYBACK_PATH_SPK="Speakers"
DEVICE_ASPECT_RATIO="4:3"

DEVICE_TEMP_SENSOR=("/sys/devices/virtual/thermal/thermal_zone0/temp")
EOF
